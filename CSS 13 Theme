local OrionLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/Kirill31312/Astereo-Library/main/Astereo%20UI%20Dev"))()
local Window = OrionLib:MakeWindow({Name = "Select Theme!", HidePremium = false, SaveConfig = false, ConfigFolder = "OrionTest"})
local Tab = Window:MakeTab({
 Name = "Themes",
 Icon = "rbxassetid://4483345998",
 PremiumOnly = false
})
Tab:AddButton({
 Name = "Red Theme",
 Callback = function()
 loadstring(game:HttpGet('https://raw.githubusercontent.com/Kirill31312/CSS-Script-V0.3-R/refs/heads/main/CSS%20Red'))()
 OrionLib:Destroy()
 end 
})
Tab:AddButton({
 Name = "Classic Theme",
 Callback = function()
 loadstring(game:HttpGet('https://raw.githubusercontent.com/Kirill31312/CSS-Script-V0.3-R/refs/heads/main/CSS%20Dev'))()
 OrionLib:Destroy()
 end 
})
